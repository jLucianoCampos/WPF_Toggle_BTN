<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" KeyDown="Window_KeyDown">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="123*"/>
            <RowDefinition Height="298*"/>
        </Grid.RowDefinitions>
        <Slider HorizontalAlignment="Left" Margin="168,0.333,0,0" VerticalAlignment="Top" Width="137" Foreground="#FFF3F3F3" RenderTransformOrigin="0,0" Grid.Row="1">
            <Slider.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform X="5"/>
                </TransformGroup>
            </Slider.RenderTransform>
        </Slider>
        <local:ToggleButton x:Name="btnPrivacy" HorizontalAlignment="Left" Height="29" Margin="496,20.333,0,0" VerticalAlignment="Top" Width="57" Grid.Row="1"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="120,75.333,0,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
